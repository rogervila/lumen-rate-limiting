{
    "name": "rogervila/lumen-rate-limiting",
    "description": " Lumen port of Laravel ThrottleRequests middleware",
    "keywords": [
        "laravel",
        "lumen",
        "rate",
        "limiter"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Vil√†",
            "email": "rogervila@me.com"
        }
    ],
    "require": {
        "laravel/lumen-framework": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LumenRateLimiting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\LumenRateLimiting\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
